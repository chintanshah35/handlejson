{
  "name": "handlejson",
  "version": "0.1.0",
  "description": "JSON, handled. Safe parse, stringify, format, validate.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "json",
    "parse",
    "stringify",
    "safe-json",
    "json-parse",
    "json-stringify",
    "circular-reference",
    "json-format",
    "json-validator",
    "json-minify",
    "error-handling",
    "typescript",
    "browser",
    "node"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chintanshah35/handlejson.git"
  },
  "bugs": {
    "url": "https://github.com/chintanshah35/handlejson/issues"
  },
  "homepage": "https://github.com/chintanshah35/handlejson#readme",
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18"
  }
}

